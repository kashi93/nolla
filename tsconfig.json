{
  "compilerOptions": {
    "types": [
      "node"
    ],
    "module": "commonjs",
    "target": "ES5",
    "noImplicitAny": false,
    "esModuleInterop": true,
    "sourceMap": false,
    "declaration": false,
    "allowJs": true,
    "noLib": false,
    "resolveJsonModule": true,
    "outDir": "./build",
    "rootDir": "./"
  },
  "include": [
    "./*.ts",
    "./*.js",
    "./tests/unit_list.json",
    "./vendor/template/model.template.balk",
    "nolla.ts"
  ]
}